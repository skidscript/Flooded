                function plrchat(plr, t)
                    print(plr.Name .. ": " .. tick() .. "\n" .. t)
                end
                for q, r in pairs(game:GetService("Players"):GetPlayers()) do
                    r.Chatted:connect(
                        function(t)
                            if chatlogs then
                                plrchat(r, t)
                            end
                        end
                    )
                end
                game:GetService("Players").PlayerAdded:connect(
                    function(plr)
                        plr.Chatted:connect(
                            function(t)
                                if chatlogs then
                                    plrchat(plr, t)
                                end
                            end
                        )
                    end
