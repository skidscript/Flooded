    local function au()
        local l = Instance.new("LocalScript", i)
        l.Parent.MouseButton1Click:Connect(
            function()
                while wait(4) do
                print("AutoButton&Code Running...")
                    local function lol()
                        for i,v in pairs(game.Workspace:GetDescendants()) do
                            if v.Name == "ClickDetector" and v.Parent.Parent.MainOutput.BrickColor ~= "Bright green" then
                                if math.random(0,1) == 1 then
                                    fireclickdetector(v,2)
                                end
                                game:GetService'RunService'.RenderStepped:wait()
                            end
                        end
                    end
                    for i,v in pairs(game.Workspace:GetDescendants()) do
                        if v:IsA("TouchTransmitter") and v.Parent.Name == "Button" then
                            firetouchinterest(v.Parent,game.Players.LocalPlayer.Character.HumanoidRootPart,1)
                            game:GetService'RunService'.RenderStepped:wait()
                            firetouchinterest(v.Parent,game.Players.LocalPlayer.Character.HumanoidRootPart,0)
                            game:GetService'RunService'.RenderStepped:wait()
                            v.Parent.Name = "Clicked"
                        end
                    end
                    lol()
                    game:GetService'RunService'.RenderStepped:wait()
                    lol()
                    game:GetService'RunService'.RenderStepped:wait()
                    lol()
                    game:GetService'RunService'.RenderStepped:wait()
                    lol()
                    game:GetService'RunService'.RenderStepped:wait()
                    lol()
                    game:GetService'RunService'.RenderStepped:wait()
                    lol()
                    game:GetService'RunService'.RenderStepped:wait()
                    lol()
                    game:GetService'RunService'.RenderStepped:wait()
                    lol()
                    game:GetService'RunService'.RenderStepped:wait()
                    lol()
                    game:GetService'RunService'.RenderStepped:wait()
                    lol()
                end
            end
        )
    end
    coroutine.wrap(au)()
    local function av()
        local l = Instance.new("LocalScript", j)
        frame = l.Parent.Parent.OrangeWood
        frame.Draggable = true
        frame.Active = true
        frame.Selectable = true
        l.Parent.MouseButton1Click:connect(
            function()
                if l.Parent.Parent.OrangeWood.Visible == false then
                    l.Parent.Text = "Close"
                    l.Parent.Parent.OrangeWood.Visible = true
                else
                    l.Parent.Text = "Open"
                    l.Parent.Parent.OrangeWood.Visible = false
                end
            end
        )
    end
    coroutine.wrap(av)()
end
