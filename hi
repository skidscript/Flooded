                                            r.Parent = I.Character
                                        end
                                    end
                                end
                            end
                            if string.sub(w.Text, 1, 9) == "flyspeed " then
                                speedfly = string.sub(w.Text, 10)
                                wait()
                                change()
                            end
                            if string.sub(w.Text, 1, 7) == "carpet " then
                                for q, r in pairs(GetPlayer(string.sub(w.Text, 8))) do
                                    local ak = Instance.new("Animation")
                                    ak.AnimationId = "rbxassetid://282574440"
                                    local al = lplayer.Character.Humanoid:LoadAnimation(ak)
                                    al:Play(.1, 1, 1)
                                    bplrr = r.Name
                                    banpl = true
                                end
                            end
                            if string.sub(w.Text, 1, 8) == "uncarpet" then
                                banpl = false
                            end
                            if string.sub(w.Text, 1, 6) == "stare " then
                                for q, r in pairs(GetPlayer(string.sub(w.Text, 7))) do
                                    staring = true
                                    stareplr = r
                                end
                            end
                            if string.sub(w.Text, 1, 7) == "unstare" then
                                staring = false
                            end
                            if string.sub(w.Text, 1, 7) == "logchat" then
                                chatlogs = true
                                game:GetService("StarterGui"):SetCore(
                                    "SendNotification",
                                    {Title = "LogChat enabled", Text = "Now logging all player chat."}
                                )
                            end
                            if string.sub(w.Text, 1, 9) == "unlogchat" then
                                chatlogs = false
                                game:GetService("StarterGui"):SetCore(
                                    "SendNotification",
                                    {Title = "LogChat disabled", Text = "Stopped logging all player chat."}
                                )
                            end
                            if string.sub(w.Text, 1, 6) == "fixcam" then
                                game:GetService("Workspace").CurrentCamera:Destroy()
                                wait(0.1)
                                game:GetService("Workspace").CurrentCamera.CameraSubject = lplayer.Character.Humanoid
                                game:GetService("Workspace").CurrentCamera.CameraType = "Custom"
                                lplayer.CameraMinZoomDistance = 0.5
                                lplayer.CameraMaxZoomDistance = 400
                                lplayer.CameraMode = "Classic"
                            end
                            if string.sub(w.Text, 1, 7) == "unstate" then
                                changingstate = false
                            end
                            w.Text = ""
                        end
                    end
                )
                wait(0.3)
                game:GetService("StarterGui"):SetCore(
                    "SendNotification",
                    {Title = "Loaded successfully!", Text = "Reviz Admin V2 by illremember"}
                )
                wait(0.1)
                print("Reviz Admin V2 loaded!")
                if game:GetService("Workspace").FilteringEnabled == true then
                    warn("FE is Enabled (Filtering Enabled)")
                    game:GetService("StarterGui"):SetCore(
                        "SendNotification",
                        {Title = "FE is Enabled", Text = "Filtering Enabled. Enjoy using Reviz Admin!"}
                    )
                else
                    warn("FE is Disabled (Filtering Disabled) Consider using a different admin script.")
                    game:GetService("StarterGui"):SetCore(
                        "SendNotification",
                        {
                            Title = "FE is Disabled",
                            Text = "Filtering Disabled. Consider using a different admin script."
                        }
                    )
                end
                local ap = Instance.new("ScreenGui")
                local v = Instance.new("Frame")
                local aq = Instance.new("ImageLabel")
                ap.Parent = game:GetService("CoreGui")
                v.Parent = ap
                v.BackgroundColor3 = Color3.new(1, 1, 1)
                v.BackgroundTransparency = 1
                v.Size = UDim2.new(1, 0, 0, 300)
                v.Position = UDim2.new(0, 0, -0.4, 0)
                aq.Parent = v
                aq.BackgroundColor3 = Color3.new(1, 1, 1)
                aq.BackgroundTransparency = 1
                aq.Position = UDim2.new(0, 0, 0, 0)
                aq.Size = UDim2.new(1, 0, 1, 0)
                aq.Image = "http://www.roblox.com/asset/?id=1542162618"
                v:TweenPosition(UDim2.new(0, 0, 0.2, 0), "Out", "Elastic", 3)
                wait(3.01)
                v:TweenPosition(UDim2.new(0, 0, 1.5, 0), "Out", "Elastic", 5)
                wait(5.01)
                ap:Destroy()
            end
        )
    end
    coroutine.wrap(k)()
    local function ar()
        local l = Instance.new("LocalScript", e)
        l.Parent.MouseButton1Click:Connect(
            function()
                while wait() do
                    game.Players.LocalPlayer.Character.Humanoid.Jump = true
                    local as = game:GetService("Players").LocalPlayer
                    if
                        game.Workspace.Hard.Entry.LiftEntryInfo.Info.Output.Text == "Game is Ready!" and
                            as.Waiting.Value == 0 and
                            as.Ingame.Value == 0
                     then
                        firetouchinterest(
                            game.Players.LocalPlayer.Character.HumanoidRootPart,
                            game:GetService("Workspace").Hard.Entry.LiftEntry,
                            1
                        )
                        game:GetService'RunService'.RenderStepped:wait()
                        firetouchinterest(
                            game.Players.LocalPlayer.Character.HumanoidRootPart,
                            game:GetService("Workspace").Hard.Entry.LiftEntry,
                            0
                        )
                        wait(8)
