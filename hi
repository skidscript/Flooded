                                        local K, L = I.Character, J.Character
                                        if K and L then
                                            K.HumanoidRootPart.CFrame = L.HumanoidRootPart.CFrame
                                        end
                                    end
                                    local function M(I, J)
                                        local K, L = I.Character, J.Character
                                        if K and L then
                                            K:MoveTo(L.Head.Position)
                                        end
                                    end
                                    H(game:GetService("Players")[r.Name], lplayer)
                                    wait(0.2)
