                speedget = 1
                lplayer = game:GetService("Players").LocalPlayer
                lplayer.CharacterAdded:Connect(
                    function(m)
                        spin = false
                        flying = false
                        staring = false
                        banpl = false
                    end
                )
                function change()
                    prefix = prefix
                    speedfly = speedfly
                end
                function GetPlayer(n)
                    local o = {}
                    local p = n:lower()
                    if p == "all" then
                        for q, r in pairs(game:GetService("Players"):GetPlayers()) do
                            table.insert(o, r)
                        end
                    elseif p == "others" then
                        for q, r in pairs(game:GetService("Players"):GetPlayers()) do
                            if r.Name ~= lplayer.Name then
                                table.insert(o, r)
                            end
                        end
                    elseif p == "me" then
                        for q, r in pairs(game:GetService("Players"):GetPlayers()) do
                            if r.Name == lplayer.Name then
                                table.insert(o, r)
                            end
                        end
                    else
                        for q, r in pairs(game:GetService("Players"):GetPlayers()) do
                            if r.Name:lower():sub(1, #n) == n:lower() then
                                table.insert(o, r)
                            end
                        end
                    end
                    return o
                end
                local s = lplayer:GetMouse()
                spin = false
                followed = false
                traill = false
                noclip = false
